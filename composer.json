{
    "name": "pouler/deezer-api",
    "description": "PHP Package for the Deezer API",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Radt",
            "email": "paulradt@gmail.com"
        }
    ],
    "keywords": [ "deezer", "api" ],
    "homepage": "https://github.com/PouleR/deezer-api",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-http/mock-client": "^1.0",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "require": {
        "php": "^7.0",
        "php-http/discovery": "^1.0",
        "php-http/client-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/curl-client": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "PouleR\\DeezerAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PouleR\\DeezerAPI\\Tests\\": "tests/"
        }
    }
}
